{
  "name": "dkmaker-function-app-tester",
  "version": "0.1.0",
  "description": "A test of responses from a Function App",
  "license": "MIT",
  "type": "module",
  "bin": {
    "function-app-tester": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
